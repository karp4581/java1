public class Person {
    private String name;
    private int age;
    private String country;
    public Person (String name1, int age1, String country) {
        name = name1;
        age = age1;
        this.country = "RU";
    }
    public String getName() {
        return name;}
    public int getAge() {
        return age;}
    public String getCountry() {
        return country;}
}